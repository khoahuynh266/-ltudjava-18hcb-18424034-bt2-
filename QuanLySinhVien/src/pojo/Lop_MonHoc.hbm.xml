<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojo.Lop_MonHoc" table="lop_monhoc" catalog="quanlysinhvien" optimistic-lock="version">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="identity" />
        </id>
        <property name="tenLopMH" type="string">
            <column name="TenLopMH" length="10" not-null="true" />
        </property>
        <property name="maSinhVien" type="string">
            <column name="MaSinhVien" length="10" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
